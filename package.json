{
  "name": "fhevm-react-template-monorepo",
  "version": "1.0.0",
  "description": "Universal FHEVM SDK for building confidential dApps",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "install:all": "npm install",
    "build": "npm run build:sdk && npm run build:examples",
    "build:sdk": "npm run build --workspace=packages/fhevm-sdk",
    "build:examples": "npm run build --workspaces --if-present",
    "dev:next": "npm run dev --workspace=examples/nextjs-app",
    "dev:vue": "npm run dev --workspace=examples/vue-app",
    "dev:node": "npm run start --workspace=examples/nodejs-app",
    "dev:literature": "npm run dev --workspace=examples/literature-review",
    "test": "npm test --workspaces --if-present",
    "test:sdk": "npm test --workspace=packages/fhevm-sdk",
    "lint": "npm run lint --workspaces --if-present",
    "lint:fix": "npm run lint:fix --workspaces --if-present",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json,md}\"",
    "clean": "npm run clean --workspaces --if-present && rm -rf node_modules",
    "typecheck": "npm run typecheck --workspaces --if-present"
  },
  "devDependencies": {
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "zama",
    "encryption",
    "privacy",
    "blockchain",
    "ethereum",
    "sdk",
    "react",
    "vue",
    "nextjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/username/fhevm-react-template"
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
